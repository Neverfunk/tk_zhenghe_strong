@BUILDING_GET_NAME_清掃局()
    #FUNCTIONS
    RETURNF "清掃局"

@BUILDING_GET_EXP_清掃局()
    #FUNCTIONS
    RETURNF "每回合部队疲劳度的恢复量增加。不重複计算"

@BUILDING_SET_INFO_ID_100()
    BUILDING_TYPE:建造物_清掃局 = 建造物_类型_特殊类
    BUILDING_LEVEL:建造物_清掃局 = 建造物_等级_一级建筑
    BUILDING_LIMIT_NUM:建造物_清掃局 = 建造物_数量限制_1
    BUILDING_LIMIT_UNI:建造物_清掃局 = 1
    BUILDING_COUNTRY_ID:建造物_清掃局:特殊勢力_ホフゴブリン = 1
    BUILDING_HAVE_SKILL:建造物_清掃局 = 0
    BUILDING_TOHO_REQ_ID:建造物_清掃局:0 = 0

@CHECK_BUILDING_FUNCTION_清掃局(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_清掃局
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_清掃局(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORM 因为《清掃局》的存在，
    PRINTFORML %ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%势力的部队疲劳值恢复了！
    RETURNF 1
