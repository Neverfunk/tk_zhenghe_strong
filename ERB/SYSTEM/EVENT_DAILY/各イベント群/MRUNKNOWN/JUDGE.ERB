@MRU_JUDGE_TEST1
	MRU_JUDGE_STATUS = 1
	MRU_JUDGE_CONDITION_NUM = 0
	VARSET MRU_JUDGE_CONDITION_STATUS,1
	IF 3 > 2
		MRU_JUDGE_CONDITION_STATUS:MRU_JUDGE_CONDITION_NUM = 0
		MRU_JUDGE_STATUS = 0
	ENDIF
	{
		MRU_JUDGE_CONDITION_REASON:MRU_JUDGE_CONDITION_NUM '=
		@"三不能大于二"
	}
	MRU_JUDGE_CONDITION_NUM++

	IF 1 > 2
