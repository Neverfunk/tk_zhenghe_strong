@BUILDING_GET_NAME_全国市场()
    #FUNCTIONS
    RETURNF "全国市场"

@BUILDING_GET_EXP_全国市场()
    #FUNCTIONS
    RETURNF "都市的收入增加。国内《独占市场》建筑的效果提高。"

@BUILDING_SET_INFO_ID_2()
    BUILDING_TYPE:建造物_全国市场 = 建造物_类型_经济类
    BUILDING_LEVEL:建造物_全国市场 = 建造物_等级_二级建筑
    BUILDING_LIMIT_NUM:建造物_全国市场 = 建造物_数量限制_1
    BUILDING_LIMIT_UNI:建造物_全国市场 = 0
    BUILDING_COUNTRY_ID:建造物_全国市场:特殊勢力_普通势力 = 1
    BUILDING_HAVE_SKILL:建造物_全国市场 = 0
    BUILDING_TOHO_REQ_ID:建造物_全国市场:0 = 0

@CHECK_BUILDING_FUNCTION_全国市场(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    #DIM TAX
    TAX = 0
    IF TMP_BUILDING_EXIST_FLAG:(COUNTRY_ID):建造物_全国市场 == 1
        TAX = 1500
    ENDIF
    RESULT:1 = TAX
    RESULT:0 = TAX
    RETURNF TAX

@BUILDING_TEXT_全国市场(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORML 因为全国市场的存在，%ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%势力获得了1500国库资金
    RETURNF 1
