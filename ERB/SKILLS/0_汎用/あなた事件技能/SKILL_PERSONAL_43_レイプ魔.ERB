;対象選択
@SKILL_0_PERSONAL_43_SETTARGET(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
#DIM 発動者
#DIM 発動番号
#DIM スキル
#DIMS ジャンル
#DIM 発動側
#DIM 発動勢力
#DIM 発動部隊
#DIM 対象側
#DIM 対象勢力
#DIM 対象部隊
;0-敌方处女
;1-敌方非处女
;2-我方处女
;3-我方非处女
#DIM 目标, 4, 5
#DIM 候选目标数
#DIM 对象
対象側 = !発動側
VARSET 目标:0:0, 0
FOR LOCAL, 0, BATTLE_COMMANDER_NUM:対象側
    对象 = BATTLE_COMMANDER:対象側:LOCAL
    IF 对象 > 0 && IS_FEMALE(对象)
        IF VIRGIN(对象)
            目标:0:(目标:0:4) = LOCAL
            目标:0:4 ++
        ELSE
            目标:1:(目标:1:4) = LOCAL
            目标:1:4 ++
        ENDIF
    ENDIF
NEXT
FOR LOCAL, 0, BATTLE_COMMANDER_NUM:発動側
    对象 = BATTLE_COMMANDER:発動側:LOCAL
    IF LOCAL:1 > 0 && IS_FEMALE(对象)
        IF VIRGIN(对象)
            目标:2:(目标:2:4) = LOCAL
            目标:2:4 ++
        ELSE
            目标:3:(目标:3:4) = LOCAL
            目标:3:4 ++
        ENDIF
    ENDIF
NEXT
候选目标数 = 目标:0:4 + 目标:1:4 + 目标:2:4 + 目标:3:4
FOR LOCAL, 0, 4
    IF 目标:LOCAL:4 && RAND:2
        COMBAT_SKILL_TARGET = LOCAL * 10 + 目标:LOCAL:(RAND(目标:LOCAL:4))
        RETURN 1
    ELSE
        候选目标数 -= 目标:LOCAL:4
        IF 候选目标数 <= 0
            IF 目标:LOCAL:4
                COMBAT_SKILL_TARGET = LOCAL * 10 + 目标:LOCAL:(RAND(目标:LOCAL:4))
                RETURN 1
            ENDIF
            BREAK
        ENDIF
    ENDIF
NEXT
RETURN 0

;発動判定
@SKILL_0_PERSONAL_43_CAN_INVOKE(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
#DIM 発動者
#DIM 発動番号
#DIM スキル
#DIMS ジャンル
#DIM 発動側
#DIM 発動勢力
#DIM 発動部隊
#DIM 対象側
#DIM 対象勢力
#DIM 対象部隊
#DIM 人员判定 = 0
#DIM 性技判定
対象側 = !発動側
SIF BATTLE_COMMANDER_NUM:対象側 == 0
    RETURN 0
FOR LOCAL, 0, BATTLE_COMMANDER_NUM:対象側
    LOCAL:1 = BATTLE_COMMANDER:対象側:LOCAL
    SIF LOCAL:1 > 0 && IS_FEMALE(LOCAL:1)
        人员判定 ++
NEXT
FOR LOCAL, 0, BATTLE_COMMANDER_NUM:発動側
    LOCAL:1 = BATTLE_COMMANDER:発動側:LOCAL
    SIF LOCAL:1 > 0 && IS_FEMALE(LOCAL:1)
        人员判定 ++
NEXT
RETURN 発動者 == MASTER && HAS_PENIS(MASTER) && 人员判定

