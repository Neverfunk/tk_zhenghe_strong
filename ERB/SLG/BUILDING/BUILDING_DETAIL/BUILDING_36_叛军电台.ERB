@CHECK_BUILDING_FUNCTION_叛军电台(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_叛军电台
    IF RESULT:1 > 0 && (IS_SP_COUNTRY(COUNTRY_ID) || GET_OWN_CITY(COUNTRY_ID) == 1)
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_叛军电台_1(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORM 因为《叛军电台》的存在，
    SIF IS_SP_COUNTRY(COUNTRY_ID)
        PRINTFORML 即使被宣为异端的%ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%势力仍能募集到部队！
    SIF !IS_SP_COUNTRY(COUNTRY_ID)
        PRINTFORML 即使身处绝境的%ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%势力仍能募集到部队！
    RETURNF 1

@BUILDING_TEXT_叛军电台_2(COUNTRY_ID,UNIT_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    #DIM UNIT_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORM 因为《叛军电台》的存在，
    PRINTFORML %ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%的军队{UNIT_ID}疲劳恢复了！
    RETURNF 1
