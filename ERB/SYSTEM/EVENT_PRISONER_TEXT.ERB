;-------------------------------------------------
;拘留
;-------------------------------------------------
@EVENT_PRISONER_TEXT0(ARG:0)

IF TALENT:(ARG:0):崩壊 || TALENT:(ARG:0):空虚
	PRINTFORML %ANAME(ARG:0)%空虚的視線在牢中彷徨
	;TranslatedMark %ANAME(ARG:0)%は牢の中で虚ろに視線を彷徨わせている
ELSE
	IF RAND:4
		IF ABL:(ARG:0):知略 >= 70
			PRINTFORML %ANAME(ARG:0)%在牢狱里老实呆着…
			;TranslatedMark %ANAME(ARG:0)%は牢の中で大人しくしている…
		ELSEIF ABL:(ARG:0):武闘 >= 70
			PRINTFORML %ANAME(ARG:0)%在監牢中暴乱着…
			;TranslatedMark %ANAME(ARG:0)%は牢の中で暴れているようだ…
		ELSEIF ABL:(ARG:0):政治 >= 70
			PRINTFORML %ANAME(ARG:0)%和監牢外的看守搭着話…
			;TranslatedMark %ANAME(ARG:0)%は牢の外にいる看守と話し込んでいる…
		ELSE
			PRINTFORML %ANAME(ARG:0)%在牢狱里看起来很不安…
			;TranslatedMark %ANAME(ARG:0)%は牢の中で不安そうにしている…
		ENDIF
	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTFORML %ANAME(ARG:0)%在牢狱里一动不动…
				;TranslatedMark %ANAME(ARG:0)%は牢の中でじっとしている…
			CASE 1
				PRINTFORML %ANAME(ARG:0)%在牢狱里抱怨着…
				;TranslatedMark %ANAME(ARG:0)%は牢の中で文句を言っている…
			CASE 2
				PRINTFORML %ANAME(ARG:0)%在牢狱里看起来很不安…
				;TranslatedMark %ANAME(ARG:0)%は牢の中で心細そうにしている…
			CASEELSE
				PRINTFORML %ANAME(ARG:0)%在牢狱里看起来很不安…
				;TranslatedMark %ANAME(ARG:0)%は牢の中で不安そうにしている…
		ENDSELECT
	ENDIF
ENDIF

