@CHECK_BUILDING_FUNCTION_寺子屋(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_CHARA_LIMIT_USING(COUNTRY_ID,建造物_寺子屋)
        RETURNF RESULT:0
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_寺子屋
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_寺子屋(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORM 因为《寺子屋》的存在，
    PRINTFORML %ANAME(26)%尽职地为居民们传授知识
    PRINTFORML %ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%国内士官的经验增加了！
    RETURNF 1
