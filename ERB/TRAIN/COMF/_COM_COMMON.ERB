	LOCAL:0 += 40

SIF TALENT:(ARG:0):恋慕
	LOCAL:0 += 20

SIF TALENT:(ARG:0):親友
	LOCAL:0 += 20

SIF TALENT:(ARG:0):恋人
	LOCAL:0 += 20

