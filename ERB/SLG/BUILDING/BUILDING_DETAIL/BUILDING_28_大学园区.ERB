@BUILDING_GET_NAME_大学园区()
    #FUNCTIONS
    RETURNF "大学园区"

@BUILDING_GET_EXP_大学园区()
    #FUNCTIONS
    RETURNF "每回合全体士官的武斗、防卫、智略经验增加。"

@BUILDING_SET_INFO_ID_28()
    BUILDING_TYPE:建造物_大学园区 = 建造物_类型_科技类
    BUILDING_LEVEL:建造物_大学园区 = 建造物_等级_二级建筑
    BUILDING_LIMIT_NUM:建造物_大学园区 = 建造物_数量限制_1
    BUILDING_LIMIT_UNI:建造物_大学园区 = 0
    BUILDING_COUNTRY_ID:建造物_大学园区:特殊勢力_普通势力 = 1
    BUILDING_HAVE_SKILL:建造物_大学园区 = 0
    BUILDING_TOHO_REQ_ID:建造物_大学园区:0 = 0

@CHECK_BUILDING_FUNCTION_大学园区(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_大学园区
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_大学园区(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORML 因为《大学园区》的存在，%ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%国内士官的武斗，防卫，智略经验增加了！
    RETURNF 1
