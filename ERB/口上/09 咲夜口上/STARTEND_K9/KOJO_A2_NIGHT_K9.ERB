;咲夜口上の使用可否設定と初期化
SIF CFLAG:咲夜:400 == 0
	CALL KOJO_ASK_RESET_K9

;口上を不使用設定なら戻る
SIF CFLAG:咲夜:400 == 1
	RETURN 0

SETCOLOR 咲夜_口上カラー

;─────────────────────────────────────── 
;○初回
;─────────────────────────────────────── 
IF CFLAG:咲夜:201 == 0

	;閨に呼ぶ口上初回呼び出しフラグオン
	CFLAG:咲夜:201 = 1

	;───────────────────────────────────── 
	;▼虚ろ
	;───────────────────────────────────── 
	IF TALENT:咲夜:空虚
		PRINTFORML 「嗯……嗯……」
		;TranslatedMark 「う……んん……」
		PRINTFORMDL 总算等到了你的到来，%ANAME(咲夜)%却坐了下来……
