@K36_NEVERMIND_EVENT_1_RATE
    IF DAY - K36_上次色色日期 <= 3
        RETURN 2 + MIN((DAY - K36_和四季的日常_主事件_上次触发日期:1),10)
    ELSE
        RETURN 1
    ENDIF

@K36_NEVERMIND_EVENT_1_STATUS
