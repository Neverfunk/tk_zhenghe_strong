;-------------------------------------------------
;キャラARG:0にペニスがあれば「ペニス」、なければ「張り型」を返す文字関数
;返す文字列を指定することも可能
;-------------------------------------------------
@BAR_NAME(ARG:0, ARGS:1 = "肉棒", ARGS:2 = "張り型")
#FUNCTIONS
LOCALS =
IF HAS_PENIS(ARG:0)
	;show descriptive part only when penis having char is alone
	IF IS_MPLY(ARG:0) && MPLY_NUM == 1 || IS_MTAR(ARG:0) && MTAR_NUM == 1
		IF !RAND:6 && TALENT:(ARG:0):童貞
			LOCALS += @"%SPLIT_RAND("纯洁的/保持童贞的/不知道女人的/未使用的", 1)%"
			;putting a space
			LOCALS += " "
		ENDIF
		IF !RAND:3
			SELECTCASE GET_PENISSIZE(ARG:0)
				CASE -2
					LOCALS += @"%SPLIT_RAND("可爱的/孩子般的/小的/未成熟的/极小的/像牙签一样的", 1)%"
				CASE -1
					LOCALS += @"%SPLIT_RAND("小的/細的/小巧玲珑的/渺小的", 1)%"
				CASE 0
					LOCALS += @"%SPLIT_RAND("坚硬的/雄壮的/有力的/勃起的", 1)%"
				CASE 1
					LOCALS += @"%SPLIT_RAND("很大的/了不起的/太棒了的/雄壮的/粗大的/坚实的/健壮的", 1)%"
				CASE 2
					LOCALS += @"%SPLIT_RAND("巨大的/非常了不起的/像马一样大的/粗壮的/惊人的/非同寻常的", 1)%"
			ENDSELECT
		ENDIF
	ENDIF

	LOCALS += BAR_NAME_GEN()
ELSE
	LOCALS = 張り型
ENDIF
RETURNF LOCALS

