			CALL K36_SHIKIEIKI_SAY(@"（都，都怪%ANAME(MASTER)%……）",1)
			CALL K36_SHIKIEIKI_SAY(@"（也不能这么想……我自己也有很大的责任……）",1)
			CALL K36_SHIKIEIKI_SAY(@"（一定要让他负起责任来！）",1)
