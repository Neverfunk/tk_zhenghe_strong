@CHECK_BUILDING_FUNCTION_外神接入点(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_SP_COUNTRY_LIMIT_USING(COUNTRY_ID,建造物_外神接入点)
        RETURNF RESULT:0
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_外神接入点
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_外神接入点(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORM 因为《外神接入点》的存在，
    PRINTFORML 《自然培养皿》的效果再次翻倍了
    RETURNF 1
