		LOCAL:2 = CHARANUM
		FOR LOCAL:0, 0, LOCAL:2
			LOCAL:1 = LOCAL:2 - LOCAL:0 - 1
			SIF IS_SP_CHARA(LOCAL:1)
				CALL DELETE_CHARA(LOCAL:1)
		NEXT

		SIF 童子勢力 >= 1
			CALL DESTROY_COUNTRY(童子勢力)

	ENDIF
