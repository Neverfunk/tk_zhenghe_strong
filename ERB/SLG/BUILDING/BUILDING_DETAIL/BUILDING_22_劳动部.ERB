@BUILDING_GET_NAME_劳动部()
    #FUNCTIONS
    RETURNF "劳动部"

@BUILDING_GET_EXP_劳动部()
    #FUNCTIONS
    RETURNF "增加一般劳动的资金收入和经验获得。"

@BUILDING_SET_INFO_ID_22()
    BUILDING_TYPE:建造物_劳动部 = 建造物_类型_政治类
    BUILDING_LEVEL:建造物_劳动部 = 建造物_等级_二级建筑
    BUILDING_LIMIT_NUM:建造物_劳动部 = 建造物_数量限制_1
    BUILDING_LIMIT_UNI:建造物_劳动部 = 0
    BUILDING_COUNTRY_ID:建造物_劳动部:特殊勢力_普通势力 = 1
    BUILDING_HAVE_SKILL:建造物_劳动部 = 0
    BUILDING_TOHO_REQ_ID:建造物_劳动部:0 = 0

@CHECK_BUILDING_FUNCTION_劳动部(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    #DIM POLITICS
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_劳动部
    IF RESULT:1 > 0
        SIF TMP_CREATE_IS_FREE_MAP_FUNC() > 0
            POLITICS = SQRT(MAX(TMP_GET_POLITICS_POWER(CFLAG:MASTER:所属),0))/10
        RESULT:1 = MIN(100,POLITICS)
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_劳动部(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORML 因为《劳动部》的存在，%ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%国内士官正常劳动的收入增加了！
    RETURNF 1
