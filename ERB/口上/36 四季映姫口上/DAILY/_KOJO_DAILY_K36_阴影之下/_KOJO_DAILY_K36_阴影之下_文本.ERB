@四季映姫_阴影之下_初见_1
    IF !CFLAG:四季_ID:面識
        IF GET_COUNTRY_BOSS(CFLAG:四季_ID:所属) == 四季_ID
            CALL K36_NORMAL_SENTENCE_IMAGE(@"刚成为这个国家官员的%ANAME(MASTER)%受到了君主的召唤")
        ELSE
            CALL K36_NORMAL_SENTENCE_IMAGE(@"刚成为这个国家官员的%ANAME(MASTER)%受到了另一位似乎是管事的官员召唤")
        ENDIF
        CALL K36_NORMAL_SENTENCE_IMAGE(@"不过听说她可是出了名的严厉，就这样去的话说不定会被大骂一通呢",1)
        CALL K36_NORMAL_SENTENCE_IMAGE(@"要去吗？")
        CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"去",1)
        CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"不去",0)
        INPUT
        CLEARLINE 1
        IF RESULT == 0
            CALL K36_NORMAL_SENTENCE_IMAGE(@"不想受无妄之灾，还是别去了")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"摸了！",1)
            KDVAR:四季_ID:四季映姫_阴影之下_进度 = -1
        ENDIF
        CALL K36_一段时间后_IMAGE
        CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%走进了宫殿的办公处，发现坐在主位上的是一位少女")
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_担忧,四季_额外_清空)
        CALL KOJO_36_IMAGE
        CALL K36_NORMAL_SENTENCE_IMAGE(@"看起来非常年轻，但%ANAME(MASTER)%早已学会了在幻想乡不以外貌猜测年龄")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"在巨椅的衬托下，少女显得非常渺小",1)
        CALL K36_NORMAL_SENTENCE_IMAGE(@"但这无法掩盖少女肃穆的神情")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"她眼神中流露的疲态让%ANAME(MASTER)%心神一颤",1)
        CALL K36_NORMAL_SENTENCE_IMAGE(@"少女看到了走来的%ANAME(MASTER)%，站了起来")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「啊，是新来的人类呢！」")
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_开心,四季_额外_害羞)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「先请教一下您的名字。」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「%ANAME(MASTER)%吗，我知道了。」")
        CALL SET_SHIKIEIKI_FACE(四季_衣服_维持上次,四季_感情_普通,四季_额外_清空)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「我的名字是四季映姬·亚玛萨那度，叫我%ANAME(四季_ID)%就行」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「『亚玛萨那度』代表了我的职位，我曾是负责审判幻想乡死者的阎魔。」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「不过现在的情况你也知道。」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「整个幻想乡正在向战火的深渊坠落。」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「规则和秩序濒临崩溃，我的任务暂时变成了维护幻想乡的和平。」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「很高兴你能选择加入我们一起维护秩序，我们现在正需要你这样的人来帮忙」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「因为大部分人都选择在这个时候放纵自己的欲望。」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「他们在死后会被惩罚的，不过我们得先确保他们会得到应有的死亡。」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「任务会很繁重呢，这也是没有办法的事情，你要做好心理准备。」")
        CALL SET_SHIKIEIKI_FACE(四季_衣服_维持上次,四季_感情_开心,四季_额外_维持上次)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「总之，好好努力吧，一起为了未来而奋斗。」")
        CALL K36_一段时间后_IMAGE
        CALL K36_NORMAL_SENTENCE_IMAGE(@"和%ANAME(四季_ID)%互相认识后谈论了一会现在的局势……")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「%ANAME(MASTER)%很博学呢，比我那位不靠谱的下属好多了……」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「那么现在……这是一些文件，请处理好再交给我。麻烦你了。」")
        CALL SET_SHIKIEIKI_FACE(四季_衣服_维持上次,四季_感情_生气,四季_额外_生气)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「在生活上有任何问题可以去麻烦%ANAME(小町_ID)%，唯独在这种事情上她会很积极。」")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%就这样被给予了繁重的工作")
    ELSE
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_开心,四季_额外_维持上次)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「啊，是%ANAME(MASTER)%，在这里的生活可好？」")
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_生气,四季_额外_生气)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「在生活上有任何问题可以去麻烦%ANAME(小町_ID)%，唯独在这种事情上她会很积极。」")
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_普通,四季_额外_维持上次)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「如果有空的话，不妨来帮我处理事务吧。」")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"最近才认识的%ANAME(四季_ID)%毫不客气地向%ANAME(MASTER)%发布了指令")
    ENDIF
    CFLAG:四季_ID:好感度 += 200
    CFLAG:四季_ID:面識 = 1
    RETURN 1
@四季映姫_阴影之下_初见_2
