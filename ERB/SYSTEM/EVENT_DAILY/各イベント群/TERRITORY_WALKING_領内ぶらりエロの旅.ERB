@EVENT_DAILY_TERRITORY_WALKING_RATE()
RETURN 50


;---------------------
;確率以外の発生判定
;---------------------
@EVENT_DAILY_TERRITORY_WALKING_DECISION()

;勢力に所属していないか捕虜だと駄目
SIF CFLAG:MASTER:所属 == 0 || CFLAG:MASTER:捕虜先 != 0
	RETURN 0

SIF DAY < 7
	RETURN 0

SIF !HAS_PENIS(MASTER)
	RETURN 0

RETURN 1

