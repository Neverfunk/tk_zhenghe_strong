;[虚ろ]状態なら口上を表示しない
IF TALENT:空虚 || CFLAG:400 == 1
	RETURN
ENDIF
IF CFLAG:400 == 0
	PRINTFORML 请选择口头
	;TranslatedMark 口上を選択してください
	PRINTL [0]不使用口头语
	;TranslatedMark [0] 口上を不使用
	PRINTL [1]军师口头
	;TranslatedMark [1] 軍師口上

	$INPUT_LOOP
	INPUT

	IF RESULT == 0
		PRINTFORMW 不使用口头
		;TranslatedMark 口上を使用しません
		CFLAG:400 = 1
		RETURN
	ELSEIF RESULT == 1
		PRINTFORMW 使用军师口头
		;TranslatedMark 軍師口上を使用します
		CFLAG:400 = 2
		PRINTFORML 合口味改变素质吗？
		;TranslatedMark 口上に合わせて素質を変更しますか？
		PRINTFORML （注意素质会相当变质）
		;TranslatedMark (注意　素質がかなり変質します）
		PRINTL [0]不更改
		;TranslatedMark [0] 変更しない
		PRINTL [1]变更
		;TranslatedMark [1] 変更する
		$INPUT_LOOP2
		INPUT
		IF RESULT == 0
			PRINTFORMW 素质就这样开始。
			;TranslatedMark 素質はそのままで開始します。
			RETURN
		ELSEIF RESULT == 1
			PRINTFORMW 改变素质。
			;TranslatedMark 素質を変更します。
			TALENT:反抗的 = 0
			TALENT:自尊心高 = 0
			TALENT:不知羞恥 = 0
			TALENT:解放 = 0

			TALENT:処女 = 1
			TALENT:自尊心低 = 1
			TALENT:胆怯 = 1
			TALENT:圧抑 = 1
		ELSE
			GOTO INPUT_LOOP2
		ENDIF
	ELSE
		GOTO INPUT_LOOP
	ENDIF
ENDIF

;-------------------------------------------------
;初回
