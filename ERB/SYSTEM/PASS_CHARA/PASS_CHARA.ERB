;------------------------------
;继承用角色选择部分
;------------------------------
@PASS_CHARA_CHARASELECT()
#DIM FIRST_LINE
#DIM 対象
#DIM 表示位置
#DIM 表示ページ
#DIM 総ページ数
#DIM キャラ数
#DIM キャラカウンタ
#DIM 表示開始位置
#DIM 表示終了位置

VARSET キャラ数
FOR LOCAL, 0, CHARANUM
	SIF IS_TOHO_CHARA(LOCAL) && LOCAL != NAME_TO_CHARA("あなた")
		キャラ数 ++
NEXT
総ページ数 = (キャラ数 - 1) / 60 + 1
表示ページ = 1


FIRST_LINE = LINECOUNT

$SHOW_LOOP

VARSET 表示位置
VARSET キャラカウンタ
表示開始位置 = (表示ページ - 1) * 60
表示終了位置 = 表示ページ * 60

CALL SINGLE_DRAWLINE
PRINTFORML 把角色的能力引継到下一週目
PRINTFORML 請選択要進行設定的角色  
PRINTBUTTON "[統一設定]", 3
PRINTBUTTON "[統一解除]", 4
PRINTL
CALL SINGLE_DRAWLINE


FOR LOCAL:0, 0, CHARANUM
	SIF !IS_TOHO_CHARA(LOCAL) || LOCAL == NAME_TO_CHARA("あなた")
		CONTINUE
	IF 表示開始位置 <= キャラカウンタ && キャラカウンタ < 表示終了位置
		IF 表示位置 % 3!= 0
			PRINT     
		ELSEIF 表示位置 > 0
			PRINTL 
		ENDIF
		SIF 引き継ぎ設定済み:LOCAL:0
			SETCOLOR 0x00FFFF
		PRINTFORM [{LOCAL + 99, 3, LEFT}]%ANAME(LOCAL), MAX_CHARANAME_LENGTH, LEFT%
		RESETCOLOR
		表示位置 ++
	ENDIF
	キャラカウンタ ++
NEXT

PRINTL 
CALL SINGLE_DRAWLINE

IF 総ページ数 >= 2
	IF 表示ページ > 1
		PRINT [  1] 察看前一頁            
	ELSE
		PRINT                             
	ENDIF
	IF 表示ページ < 総ページ数
		PRINT [  2] 察看後一頁
	ENDIF
	PRINTL 
	CALL SINGLE_DRAWLINE
ENDIF

PRINTL [  0] 這樣就好

REDRAW 0


$INPUT_LOOP
INPUT
IF RESULT == 1 && 表示ページ > 1
	表示ページ --
	CLEARLINE LINECOUNT - FIRST_LINE
	GOTO SHOW_LOOP
ELSEIF RESULT == 2 && 表示ページ < 総ページ数
	表示ページ ++
	CLEARLINE LINECOUNT - FIRST_LINE
	GOTO SHOW_LOOP
ELSEIF RESULT == 3
	CALL PASS_CHARA_UI(-1)
	RESTART
ELSEIF RESULT == 4
	CALL CLEAR_ALL_PASS_SETTINGS
	RESTART
ELSEIF RESULT == 0
	RETURN
ENDIF

対象 = RESULT - 99
IF 対象 > CHARANUM || 対象 < 0 || !IS_TOHO_CHARA(対象) || 対象 == NAME_TO_CHARA("あなた")
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF
CALL PASS_CHARA_UI(対象)
RESTART


