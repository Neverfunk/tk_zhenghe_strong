	TIMES CUP:(ARG:0):欲情, 0.8
ENDIF

IF TALENT:(ARG:0):老実
	TIMES CUP:(ARG:0):欲情, 0.8
ENDIF

IF TALENT:(ARG:0):冷漠
	TIMES CUP:(ARG:0):欲情, 0.7
ENDIF

IF TALENT:(ARG:0):感情缺乏
	TIMES CUP:(ARG:0):欲情, 0.7
ENDIF

