	CASE "恨"
		LOCAL:0 -= 50
	CASE "怒"
		LOCAL:0 -= 20
	CASE "憤"
		LOCAL:0 -= 8
	CASE "鬱"
		LOCAL:0 -= 40
	CASE "悲"
		LOCAL:0 -= 10
	CASE "憂"
		LOCAL:0 -= 5
	CASE "狂"
		LOCAL:0 -= 40
	CASE "恐"
		LOCAL:0 -= 10
