@BUILDING_GET_EXP_理解间()
    #FUNCTIONS
    RETURNF "调教陷落外交索取时的收益增加。"

@BUILDING_SET_INFO_ID_51()
    BUILDING_TYPE:建造物_理解间 = 建造物_类型_色情类
    BUILDING_LEVEL:建造物_理解间 = 建造物_等级_二级建筑
    BUILDING_LIMIT_NUM:建造物_理解间 = 建造物_数量限制_1
    BUILDING_LIMIT_UNI:建造物_理解间 = 0
    BUILDING_COUNTRY_ID:建造物_理解间:特殊勢力_普通势力 = 1
    BUILDING_COUNTRY_ID:建造物_理解间:特殊勢力_野盗 = 1
    BUILDING_COUNTRY_ID:建造物_理解间:特殊勢力_ホフゴブリン = 1
    BUILDING_COUNTRY_ID:建造物_理解间:特殊勢力_外来人 = 1
    BUILDING_COUNTRY_ID:建造物_理解间:特殊勢力_触手 = 1
    BUILDING_COUNTRY_ID:建造物_理解间:特殊勢力_自警団 = 1
    BUILDING_COUNTRY_ID:建造物_理解间:特殊勢力_サキュバス = 1
    BUILDING_COUNTRY_ID:建造物_理解间:特殊勢力_狂信者 = 1
    BUILDING_HAVE_SKILL:建造物_理解间 = 0
    BUILDING_TOHO_REQ_ID:建造物_理解间:0 = 0

@CHECK_BUILDING_FUNCTION_理解间(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_理解间
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_理解间(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F(COLOR_色情建筑)
    PRINTFORM 因为《理解间》的存在，
    PRINTFORML 她依然完全理解获得快乐的快乐了
    PRINTFORML 对调教完成的君主进行的外交索取的收益提高了
    RETURNF 1
