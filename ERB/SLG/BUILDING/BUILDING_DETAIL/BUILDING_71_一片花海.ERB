@BUILDING_GET_NAME_一片花海()
    #FUNCTIONS
    RETURNF "一片花海"

@BUILDING_GET_EXP_一片花海()
    #FUNCTIONS
    RETURNF "美丽的花海"

@BUILDING_SET_INFO_ID_71()
    BUILDING_TYPE:建造物_一片花海 = 建造物_类型_幻想类
    BUILDING_LEVEL:建造物_一片花海 = 建造物_等级_不可建造
    BUILDING_LIMIT_NUM:建造物_一片花海 = 建造物_数量限制_无
    BUILDING_LIMIT_UNI:建造物_一片花海 = 0
    BUILDING_HAVE_SKILL:建造物_一片花海 = 0
    BUILDING_TOHO_REQ_ID:建造物_一片花海:0 = 34

@CHECK_BUILDING_FUNCTION_一片花海(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_CHARA_LIMIT_USING(COUNTRY_ID,建造物_一片花海)
        RETURNF RESULT:0
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_一片花海
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_一片花海(CITY_ID)
    #FUNCTION
    #DIM CITY_ID

    SIF !CHECK_BUILDING_TEXT_OUTPUT(CITY_OWNER:CITY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORM 因为《一片花海》的存在，
    PRINTFORML 沐浴在花海中的%CITY_NAME:CITY_ID%获得了大量的经济和国库收入
    PRINTFORML %CITY_NAME:CITY_ID%的收入和经济增加了！
    RETURNF 1
