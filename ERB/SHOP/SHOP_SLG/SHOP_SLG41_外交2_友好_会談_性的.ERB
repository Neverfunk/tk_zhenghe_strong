	LOCAL:1 = LIMIT(CFLAG:提供元:好感度 / 10, 200, 1000)
	LOCAL:2 = LIMIT(CFLAG:提供元:従属度 / 10, 200, 1000)
	IF CHECK_BUILDING_FUNCTION_SELECT(-1,CFLAG:MASTER:所属,建造物_永恒间) <= 0
		CFLAG:提供元:好感度 -= LIMIT(CFLAG:提供元:好感度 / 10, 200, 1000)
		CFLAG:提供元:従属度 -= LIMIT(CFLAG:提供元:従属度 / 10, 200, 1000)
		PRINTL
		PRINTFORM %ANAME(提供元), 8%:
		PRINTFORM 好感度%TOSTR_SIGN(LOCAL:1 * - 1)%{ABS(LOCAL:1)}(合計:{CFLAG:提供元:好感度})
		PRINTFORM 従属度%TOSTR_SIGN(LOCAL:2 * - 1)%{ABS(LOCAL:2)}(合計:{CFLAG:提供元:従属度})
		PRINTL
	ELSE
		CFLAG:提供元:好感度 += LIMIT(CFLAG:提供元:好感度 / 10, 200, 1000)
		CFLAG:提供元:従属度 += LIMIT(CFLAG:提供元:従属度 / 10, 200, 1000)
		PRINTL
		PRINTFORM %ANAME(提供元), 8%:
		PRINTFORM 好感度%TOSTR_SIGN(LOCAL:1 * + 1)%{ABS(LOCAL:1)}(合計:{CFLAG:提供元:好感度})
		PRINTFORM 従属度%TOSTR_SIGN(LOCAL:2 * + 1)%{ABS(LOCAL:2)}(合計:{CFLAG:提供元:従属度})
		PRINTL
	ENDIF
ENDIF

RETURN 満足度

