@K36_SP_ENEVT_19
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「诶居然只有%ANAME(小町_ID)%？」",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「%K36_CALL_四季大人()%呢？」",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%来到办公室，却发现%ANAME(四季_ID)%并没有来")
    CALL K35_ONOZUKA_SAY_IMAGE(@"「不知道呢，我居然有一天也能比%K36_CALL_四季大人()%来的早呢！」")
    CALL K35_ONOZUKA_SAY_IMAGE(@"「嘿嘿嘿……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"去%ANAME(四季_ID)%自己的房间找一下吧。")
    CALL K36_一段时间后_IMAGE
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%来到了%ANAME(四季_ID)%的房门口")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"里面传来了苦闷的声音",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_发情,四季_额外_清空,四季_额外_害羞)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「哈……不行……但是……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%推开了一条门缝")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"发现%ANAME(四季_ID)%正坐在座位上",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"面前的桌上摊开着散乱的文件")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"不过%ANAME(四季_ID)%的注意力并不在桌上……",1)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「再忍一下……哈……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"虽然嘴上这么说着，%ANAME(四季_ID)%的双手却向身下伸去",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"双腿颤抖着把手夹在大腿的内侧")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"手微微抽动着，获取着快感",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_发情,四季_额外_清空,四季_额外_害羞重,四季_额外_吐气)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「唔……明明……哈……要忍不住了……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%把身体压到桌子上")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"右手抽了了出来，开始揉搓着自己的乳房",1)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「哈……%ANAME(MASTER)%……唔……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"左手则开始爱抚着私处的周边")
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_发情,四季_额外_清空,四季_额外_害羞重,四季_额外_吐气,四季_额外_爱心)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「唔……哈……那里……要……要去了……」")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「%K36_CALL_MASTER()%！啊啊啊啊——」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%浑身颤抖着瘫软了下来")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"头侧压在桌上，口水不受控制的流了下来",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_流泪,四季_额外_清空,四季_额外_害羞重,四季_额外_吐气)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「明明没有%K36_CALL_MASTER()%的舒服……还是去了呢……」")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「欲望……已经要溢出来了……但是……」")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「%ANAME(MASTER)%明明这么努力……我却在这里……对不起……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"自亵后一副不成体统模样的%ANAME(四季_ID)%厌恶地开始责怪起自己来",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%趁着被发现前溜走了")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"有空还是好好疼爱%ANAME(四季_ID)%一下吧？",1)
    RETURN 1
