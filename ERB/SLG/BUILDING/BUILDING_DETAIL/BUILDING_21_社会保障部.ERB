@BUILDING_GET_NAME_社会保障部()
    #FUNCTIONS
    RETURNF "社会保障部"

@BUILDING_GET_EXP_社会保障部()
    #FUNCTIONS
    RETURNF "回合结束时，给予国内低级士官大量属性经验。"

@BUILDING_SET_INFO_ID_21()
    BUILDING_TYPE:建造物_社会保障部 = 建造物_类型_政治类
    BUILDING_LEVEL:建造物_社会保障部 = 建造物_等级_二级建筑
    BUILDING_LIMIT_NUM:建造物_社会保障部 = 建造物_数量限制_1
    BUILDING_LIMIT_UNI:建造物_社会保障部 = 0
    BUILDING_COUNTRY_ID:建造物_社会保障部:特殊勢力_普通势力 = 1
    BUILDING_HAVE_SKILL:建造物_社会保障部 = 0
    BUILDING_TOHO_REQ_ID:建造物_社会保障部:0 = 0

@CHECK_BUILDING_FUNCTION_社会保障部(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    #DIM POLITICS
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_社会保障部
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_社会保障部(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORM 因为《社会保障部》的存在，
    PRINTFORML %ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%国内士官的全经验增加了！
    RETURNF 1
