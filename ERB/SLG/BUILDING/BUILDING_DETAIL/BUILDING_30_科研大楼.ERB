@BUILDING_SET_INFO_ID_30()
    BUILDING_TYPE:建造物_科研大楼 = 建造物_类型_科技类
    BUILDING_LEVEL:建造物_科研大楼 = 建造物_等级_二级建筑
    BUILDING_LIMIT_NUM:建造物_科研大楼 = 建造物_数量限制_1
    BUILDING_LIMIT_UNI:建造物_科研大楼 = 0
    BUILDING_COUNTRY_ID:建造物_科研大楼:特殊勢力_普通势力 = 1
    BUILDING_HAVE_SKILL:建造物_科研大楼 = 0
    BUILDING_TOHO_REQ_ID:建造物_科研大楼:0 = 0

@CHECK_BUILDING_FUNCTION_科研大楼(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_科研大楼
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0


