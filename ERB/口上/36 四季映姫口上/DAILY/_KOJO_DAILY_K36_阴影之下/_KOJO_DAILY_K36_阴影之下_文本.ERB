        CALL K36_一段时间后_IMAGE
        CALL SET_ONOZUKA_FACE(小町_衣服_普通,小町_感情_开心,小町_额外表情_无)
        CALL K35_ONOZUKA_SAY_IMAGE(@"「%ANAME(MASTER)%?那么，请多指教了%ANAME(MASTER)%！」")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(小町_ID)%在说完后再次如风一般离开了")
        CBGCLEAR
        CALL K36_NORMAL_SENTENCE_IMAGE(@"（……）",1)
    ENDIF
    CFLAG:小町_ID:好感度 += 200
    CFLAG:小町_ID:面識 = 1
