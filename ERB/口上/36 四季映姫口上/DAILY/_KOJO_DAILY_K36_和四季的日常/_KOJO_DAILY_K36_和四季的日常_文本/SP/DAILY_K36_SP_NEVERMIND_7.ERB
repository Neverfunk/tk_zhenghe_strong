@K36_SP_ENEVT_7_EXIST
    RETURN 1

@K36_SP_ENEVT_7_STATUS
    SIF K36_和四季的日常_四季好感度 < 40 || K36_和四季的日常_四季敏感度 < 40
        RETURN 0
    RETURN 1

@K36_SP_ENEVT_7_NAME
    ; CLEARLINE 1
    ; CALL KOJO_DAILY_START("秘密婚礼")
    RETURN 1

@K36_SP_ENEVT_7
    #DIM CHOICE,4
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%在忙碌的工作中罕见的闲了下来")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"因为交接的问题",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"本来应该继续忙碌的%ANAME(MASTER)%不得不无所事事地等待着")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"闭上眼想着休息一会的%ANAME(MASTER)%",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"眼前却冒出了%ANAME(四季_ID)%的身影")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"（幻想一下也不错呢）",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%畅想着着和%ANAME(四季_ID)%一起正常生活的情形")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"突然间一段思绪跳上了%ANAME(MASTER)%的心头",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"（想和%K36_CALL_四季大人()%结婚！）")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"（等下……打仗的时候想这个未免过于不吉利了……）",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"但是思绪这种东西……一旦起头就无法遏制了")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"（反正……只是妄想呢……）",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"（想和%ANAME(四季_ID)%在哪里结婚呢）",1)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"中有之道",0)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"再思之道",0)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"三途川边",0)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"阳间一点的地方！",1)
    INPUT
    CLEARLINE 1
    IF RESULT
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「果然结婚地点还是得选人里或者神社吧，不知道%K36_CALL_映姬()%能不能接受呢」",1)
        CHOICE:0 = 1
    ELSE
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「虽然很诡异，但是要和阎魔结婚还是得在地狱这种地方吧？」",1)
        CHOICE:0 = 0
    ENDIF
    CALL K36_NORMAL_SENTENCE_IMAGE(@"（服装也是很重要的吧）")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"（想让%K36_CALL_四季大人()%穿什么呢）")
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"凤冠霞帔",3)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"白无垢",2)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"西式婚纱",1)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"常服就好",0)
    INPUT
    CLEARLINE 1
    IF RESULT == 3
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「当然得是凤冠霞帔！就是不知道幻想乡能不能定到……」",1)
        CHOICE:1 = 3
    ELSEIF RESULT == 2
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「白无垢！无需多言的选择！」",1)
        CHOICE:1 = 2
    ELSEIF RESULT == 1
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「或许西服什么的也不错？律师服也行嘛（笑）」",1)
        CHOICE:1 = 1
    ELSE
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「普通的衣服也不错！就得是这个味！」",1)
        CHOICE:1 = 0
    ENDIF
    CALL K36_NORMAL_SENTENCE_IMAGE(@"（还有朋友……）")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"（要邀请谁呢？）")
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"老子打下幻想乡就是想让你们全部参加我的婚礼的",3)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"朋友们来，都可以来",2)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"带上我们忠诚的%ANAME(小町_ID)%作为见证人",1)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"JUST SHIKIEIKI",0)
    INPUT
    CLEARLINE 1
    IF RESULT == 3
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「所有人都得来！结完婚正好大赦天下！」",1)
        CHOICE:2 = 3
    ELSEIF RESULT == 2
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「现场教学怎么拿魔理沙的舌头做三菜一汤！」",1)
        CHOICE:2 = 2
    ELSEIF RESULT == 1
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「需要一个人来捧哏！只能是你了%ANAME(小町_ID)%」",1)
        CHOICE:2 = 1
    ELSE
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「只要%K36_CALL_四季大人()%就够了！」",1)
        CHOICE:2 = 0
    ENDIF
    CALL K36_NORMAL_SENTENCE_IMAGE(@"（说起来，之前的老婆怎么办呢？）")
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"但这是碰都不能碰的话题",2)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"%K36_CALL_四季大人()%不死，尔等皆是小妾",1)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"你们都是我的翅膀",0)
    INPUT
    CLEARLINE 1
    IF RESULT == 2
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「给他们一点小小的ERA震感！」",1)
        CHOICE:3 = 2
    ELSEIF RESULT == 1
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「超越寿命论存在的含金量！」",1)
        CHOICE:3 = 1
    ELSE
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「我全都要！」",1)
        CHOICE:3 = 0
    ENDIF
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「这样的婚礼……光是想想就是至福……」",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_开心,四季_额外_清空,四季_额外_害羞)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「什么是至福呀？」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"不知何时%ANAME(四季_ID)%来到了%ANAME(MASTER)%的身后")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「诶？%K36_CALL_映姬()%？」",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「能为%K36_CALL_四季大人()%效力是至福呢！」",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_戏谑,四季_额外_清空,四季_额外_害羞)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「……这样啊……那婚礼还办吗？」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「当然办……啊！」",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「咳……」",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_睡着,四季_额外_清空,四季_额外_害羞)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%默默地坐在了%ANAME(MASTER)%的怀中")
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_爱意,四季_额外_清空,四季_额外_害羞)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「%K36_CALL_MASTER()%……是认真的么？」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「当然！我一直都想……和%K36_CALL_映姬()%结婚」",1)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「是不是有点唐突了！」",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「虽然我很想但是不会强迫%K36_CALL_四季大人()%的！」",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%把头靠在了%ANAME(MASTER)%的胸口，抬头向上望着")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"淡淡的泪珠从%ANAME(四季_ID)%的眼眶中慢慢地滑落")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「诶！%K36_CALL_映姬()%……怎么……」",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_开心,四季_额外_清空,四季_额外_害羞)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「因为很高兴呢……%K36_CALL_MASTER()%愿意……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%微笑着说")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「我还担心像%ANAME(MASTER)%这种花心的人不会想这种事情……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「怎么可能！我一直都很想和%K36_CALL_四季大人()%结婚的！」",1)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「嘿嘿嘿……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%把身体更深地沉浸在%ANAME(MASTER)%的怀抱中")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「光是想想……就很幸福了呢……」")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「%ANAME(MASTER)%想要什么样的婚礼呢？」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「唔……最好的！只要%K36_CALL_映姬()%喜欢！」",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_戏谑,四季_额外_清空,四季_额外_害羞)
    IF CHOICE:0
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「不是要在人里或者神社附近结婚吗？」")
    ELSE
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「不是要在地狱附近结婚吗？」")
    ENDIF
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「诶……这不是全被听到了吗！」",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_开心,四季_额外_清空,四季_额外_害羞)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「嘿嘿嘿……」")
    IF CHOICE:1
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「是要穿外界的服装吗……不知道幻想乡里能不能找到呢。」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「不过%ANAME(MASTER)%想要的话我肯定会答应的！」")
    ELSE
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「婚纱用常服的话感觉不太正式呢……」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「但是我喜欢，有一种很随性的幸福感！」")
    ENDIF
    IF CHOICE:2
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「邀请谁来都可以呢！」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「不过千万不要把准备工作留给%ANAME(小町_ID)%就行！」")
    ELSE
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「婚礼只有我们两个人什么的……」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「其实我一直期望的就是那样的呢……」")
    ENDIF
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%开心地复述着刚刚听到的%ANAME(MASTER)%流露的心声")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「……」")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「谢谢你……%K36_CALL_MASTER()%……我真的很开心呢！」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%转过身来，坐在%ANAME(MASTER)%的腿上")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"尚且带着泪珠地凝视着%ANAME(MASTER)%的双眼")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「……」")
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_睡着,四季_额外_清空,四季_额外_害羞)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「……」")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「……」")
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_爱意,四季_额外_清空,四季_额外_害羞)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「不过结婚仪式什么的还是太麻烦了呢！」")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「不仅贵还要花时间……」")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「所以说……能和%ANAME(MASTER)%心意相通我已经很满足了！」")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「仪式什么的反而没那么重要了……」")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「如果%K36_CALL_MASTER()%愿意的话……就在这里娶我吧……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%决定")
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"只有两个人的婚礼",1)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"一定要明媒正娶！",0)
    INPUT
    CLEARLINE 1
    IF RESULT
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_开心,四季_额外_清空,四季_额外_害羞)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「请让我成为你的妻子吧！%K36_CALL_MASTER()%！」")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「%K36_CALL_映姬()%！请成为我的妻子吧！」",1)
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_接吻,四季_额外_清空,四季_额外_害羞)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「嘿嘿嘿……一直想这么说了呢……」")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%压上了%ANAME(MASTER)%，两人长吻了起来")
        CALL K36_NORMAL_SENTENCE_IMAGE_NOTICE(@" %ANAME(四季_ID)%成为了%ANAME(MASTER)%的妻子！",1)
        TALENT:四季_ID:正妻 = 1
    ELSE
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「是……这样吗？」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「嘿嘿嘿！那我还是好好期待那一天吧！」")
        CALL K36_NORMAL_SENTENCE_IMAGE_NOTICE(@" 和%ANAME(四季_ID)%许下了一定会娶她的约定",1)
    ENDIF
    RETURN 1
