@LIFE_BUILDING_ADD_EROWORKING_INCOME(ERO_INCOME)
    #FUNCTION
    #DIM ERO_INCOME
    #DIM COUNTRY_ID
    COUNTRY_ID = CFLAG:MASTER:所属
    IF CHECK_BUILDING_FUNCTION_SELECT(-1,COUNTRY_ID,建造物_小型妓院) > 0
        ERO_INCOME = ERO_INCOME * (100 + (RESULT:1)*10) / 100
        BUILDING_TEXT = BUILDING_TEXT_小型妓院(COUNTRY_ID)
    ENDIF

    IF CHECK_BUILDING_FUNCTION_SELECT(-1,COUNTRY_ID,建造物_淫腐街) > 0
        ERO_INCOME = ERO_INCOME * 200 / 100
        BUILDING_TEXT = BUILDING_TEXT_淫腐街(COUNTRY_ID)
    ENDIF

    IF CHECK_BUILDING_FUNCTION_SELECT(-1,COUNTRY_ID,建造物_国家大妓院) > 0
        ERO_INCOME = ERO_INCOME * 300 / 100
        BUILDING_TEXT = BUILDING_TEXT_国家大妓院(COUNTRY_ID)
    ENDIF

    IF CHECK_BUILDING_FUNCTION_SELECT(-1,COUNTRY_ID,建造物_旧淫腐街) > 0
        BUILDING_TEXT = BUILDING_TEXT_旧淫腐街_1(COUNTRY_ID)
        ERO_INCOME = ERO_INCOME * 500 / 100
    ENDIF
    RETURNF ERO_INCOME


@LIFE_BUILDING_ADD_WORKING_INCOME(NORMAL_INCOME)
    #FUNCTION
    #DIM NORMAL_INCOME
    #DIM COUNTRY_ID
    COUNTRY_ID = CFLAG:MASTER:所属
    IF CHECK_BUILDING_FUNCTION_SELECT(-1,COUNTRY_ID,建造物_劳动部) > 0
        NORMAL_INCOME = NORMAL_INCOME * (100 + RESULT:1) / 100
        BUILDING_TEXT = BUILDING_TEXT_劳动部(COUNTRY_ID)
    ENDIF
    RETURNF NORMAL_INCOME

