	RETURN 0

;好感度が500以上
SIF MAX(CFLAG:対象:好感度) < 500
	RETURN 0

SIF KDVAR:対象:霖之助_マジックアイテム作成 == -1
	RETURN 0

;お金を2500以上持ってないと駄目
SIF MONEY <= 2500
	RETURN 0

RETURN CHECK_KOJO_DAILY_HAPPEN(対象, 1, 0, -1)

