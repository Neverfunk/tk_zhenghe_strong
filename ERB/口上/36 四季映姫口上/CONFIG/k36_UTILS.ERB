@K36_NORMAL_PRINTDATA_IMAGE(SENTENCE = "",PRINT_FLAG = 0,WAIT_FLAG = 0)
    #DIMS SENTENCE
    #DIMS SENTENCE_LIST,100
    #DIM SENTENCE_NUM = 0
    #DIM WAIT_FLAG
    #DIM PRINT_FLAG
    IF SENTENCE != ""
        SENTENCE_LIST:SENTENCE_NUM '= SENTENCE
        SENTENCE_NUM++
    ENDIF
    IF PRINT_FLAG && SENTENCE_NUM > 0
