;--------------------------------------------------------
;@FLAG_STRBODY
;各種フラグを記録
;--------------------------------------------------------
@FLAG_STRBODY(ARGS)
#FUNCTION
#DIM LCOUNT
#DIM 分割数
#DIMS 体位候補, 15
#DIMS 体位

VARSET 体位候補
VARSET LOCAL
VARSET LOCALS

SPLIT ARGS, "：", LOCALS
分割数 = RESULT

FOR LCOUNT, 0, 分割数
	LOCALS:11 = %LOCALS:LCOUNT%
	SELECTCASE LOCALS:11
		CASE "正常位", "後背位", "騎乗位", "背面騎乗位", "対面座位", "背面座位", "側位", "屈曲位"
			IF LOCALS:13 == ""
				LOCALS:13 '= LOCALS:11
			ELSE
				LOCALS:13 '= LOCALS:13 + "：" + LOCALS:11
			ENDIF
		CASEELSE
			SETBIT LOCAL:0, NUM_STRBODY(LOCALS:11)
	ENDSELECT
NEXT


;体位候補からランダムに一つを選ぶ
IF LOCALS:13 != ""
	SPLIT LOCALS:13, "：", 体位候補
	LOCAL:1 = RESULT
	体位 = %体位候補:(RAND:(LOCAL:1))%
	SETBIT LOCAL:0, NUM_STRBODY(体位)
ENDIF

;連動するものをここで設定しておく
SIF CHECK_STRBODY("愛撫", LOCAL:0)
	SETBIT LOCAL:0, NUM_STRBODY("接触")	


RETURNF LOCAL:0

