@CALCU_NOWEX_ORGASM(ARG:0)

FOR LOCAL:0, 0, 100
	SOURCE:(ARG:0):(LOCAL:0) = LIMIT(SOURCE:(ARG:0):(LOCAL:0), 0, 100000000)
NEXT

FOR LOCAL:0, 0, 10
	TRYCALLFORM CALC_SOURCE{LOCAL:0}(ARG:0)
NEXT

CALL UP_TALENT_CHECK1(ARG:0)

CALL UP_DATE_CHECK1(ARG:0)

VARSET LOCAL, 0

FOR LOCAL:0, 0, 6
	LOCAL:2 = PALAM:(ARG:0):(LOCAL:0) + CUP:(ARG:0):(LOCAL:0)
	IF LOCAL:2 >= 10000
		IF LOCAL:2 >= 150000
			NOWEX:(ARG:0):(LOCAL:0) = 9
		ELSEIF LOCAL:2 >= 80000
			NOWEX:(ARG:0):(LOCAL:0) = 4
		ELSEIF LOCAL:2 >= 30000
			NOWEX:(ARG:0):(LOCAL:0) = 2
		ELSE
			NOWEX:(ARG:0):(LOCAL:0) = 1
		ENDIF
	ENDIF
NEXT

@CHECK_VERGIN_LOST_SITUATION(ARG:0)
