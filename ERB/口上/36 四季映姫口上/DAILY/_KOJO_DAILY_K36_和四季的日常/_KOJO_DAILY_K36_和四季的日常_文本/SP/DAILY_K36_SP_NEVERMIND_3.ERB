@K36_SP_ENEVT_3_EXIST
    RETURN 1

@K36_SP_ENEVT_3_STATUS
    SIF K36_和四季的日常_四季敏感度 < 20
        RETURN 0
    RETURN 1

@K36_SP_ENEVT_3_NAME
    CLEARLINE 1
    CALL KOJO_DAILY_START("人欲胜天")
    RETURN 1

@K36_SP_ENEVT_3
    CALL SET_SHIKIEIKI_FACE(四季_衣服_衬衫,四季_感情_接吻,四季_额外_清空,四季_额外_害羞,四季_额外_吐气)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「这位先生……要来做吗？」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%一个人来到了盗匪的据点外",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"露出一股罕见的媚态，尝试着勾引看守的盗贼")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「人家想要了啦……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%摆出了一副欲求不满的样子")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「看样子你一个人也满足不了我呢呼呼呼」")
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「为什么不再叫几个人呢……」")
    CBGCLEAR
    CALL K36_一段时间后_IMAGE
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%来到现场时已经是一片狼藉了",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"那些让当地治安官头疼了很久的盗匪们",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"已经被很均匀地镶在了小巷的各个墙面上……",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_衬衫,四季_感情_开心,四季_额外_清空,四季_额外_害羞)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「怎么样%ANAME(MASTER)%？我就说能行吧？」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「厉害……」",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_衬衫,四季_感情_普通,四季_额外_清空)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「这样就是一网打尽了呢……据点那里应该没有漏网之鱼吧？」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「检查过了，没有……」",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「这绝对算钓鱼执法吧！」",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_衬衫,四季_感情_开心,四季_额外_清空)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「嗯哼~但是无罪。」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%充满钦佩地看着大大小小的被勾引出来的盗贼")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「这个人都是花甲之年了吧，都被你勾引出来了……」",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_衬衫,四季_感情_生气,四季_额外_清空,四季_额外_害羞)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「什么叫勾引啊，真是的。」")
    IF GET_ONOZUKA_STATUS() > 0
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「要不我们问问%ANAME(小町_ID)%吧？」",1)
    ELSE
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「要不我们问问其他伙伴吧？」",1)
    ENDIF
    CALL SET_SHIKIEIKI_FACE(四季_衣服_衬衫,四季_感情_生气,四季_额外_清空,四季_额外_害羞重)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「你敢！」")
    CALL SET_SHIKIEIKI_FACE(四季_衣服_衬衫,四季_感情_担忧,四季_额外_清空,四季_额外_害羞重)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「唔……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"「总算察觉过来刚刚的行为有多羞耻了吗……我也吓了一跳呢……」",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_衬衫,四季_感情_爱意,四季_额外_清空,四季_额外_害羞重)
    CALL K36_SHIKIEIKI_SAY_IMAGE(@"「……呜……%K36_CALL_MASTER()%刚刚是怎么看的呢？」")
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"硬的不行（拳头）",1)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"硬的不行（下面）",0)
    INPUT
    CLEARLINE 1
    IF RESULT
        CALL SET_SHIKIEIKI_FACE(四季_衣服_衬衫,四季_感情_担忧,四季_额外_清空,四季_额外_害羞重)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「唔……对不起……不要生气啊……」")
        CALL SET_SHIKIEIKI_FACE(四季_衣服_衬衫,四季_感情_爱意,四季_额外_清空,四季_额外_害羞重)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「我下次不做了……」")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「嘛……至少没有实质上的损失嘛。」",1)
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「不过一定要小心呢……」",1)
        CALL SET_SHIKIEIKI_FACE(四季_衣服_衬衫,四季_感情_开心,四季_额外_清空,四季_额外_害羞)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「我就知道你会这么说！嘿嘿嘿……」")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「你还上瘾了吗！」",1)
        CALL K36_NORMAL_SENTENCE_IMAGE(@"两人一边拌着嘴一边返回了据点")
    ELSE
        CALL SET_SHIKIEIKI_FACE(四季_衣服_衬衫,四季_感情_生气,四季_额外_清空,四季_额外_害羞,四季_额外_生气)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「还真是……我鄙视你！」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「你的欲望原来这么肮脏吗！」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「你这样子迟早哪天得被像我这样的人弄死！」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「回去好好反省！」")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%气鼓鼓地走了")
    ENDIF
    CALL K36_NORMAL_SENTENCE_IMAGE_NOTICE(@" %ANAME(四季_ID)%习得了技能 《人欲胜天》")
    CALL SKILL_LEARN_BY_NAME(四季_ID, スキル_ジャンル_PERSONAL, NO:四季_ID, "人欲胜天")
    RETURN 1
