@EVENT_DAILY_FUND_RAISING_TAG()
	LOCAL = 0
	RETURN LOCAL
;---------------------
;発生確率(1000分率 100で10%)
;---------------------
@EVENT_DAILY_FUND_RAISING_RATE()
RETURN (30 + INRANGE(DVAR:募金_抱いた回数, 0, 10) * 7)


;---------------------
;確率以外の発生判定
;---------------------
@EVENT_DAILY_FUND_RAISING_DECISION()
SIF DAY < 10
	RETURN 0
;資金が50000以上の場合
SIF MONEY < 50000
	RETURN 0
RETURN 1

