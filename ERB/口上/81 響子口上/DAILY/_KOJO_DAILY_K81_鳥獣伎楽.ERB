#DIM 対象
#DIM ミスティア
ミスティア = NAME_TO_CHARA("ミスティア")

;ミスティアが自軍にいないと駄目
SIF !(CFLAG:ミスティア:所属 == CFLAG:MASTER:所属)
	RETURN 0

;主人公がミスティアだと駄目
SIF MASTER == NAME_TO_CHARA("ミスティア")
	RETURN 0

;対象が女でないとだめ
SIF !IS_FEMALE(対象)
	RETURN 0

;主人公と同一勢力で捕虜でなく、好感度500以上
RETURN CHECK_KOJO_DAILY_HAPPEN(対象, 1, 0) && CFLAG:対象:好感度 >= 500

