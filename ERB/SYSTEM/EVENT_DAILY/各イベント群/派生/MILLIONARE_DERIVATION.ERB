;---------------------
;対応するデイリーのDISABLEを返す。設定しない場合、イベントは発生しない。
;---------------------
@EVENT_DAILY_DERIVATION_MILLIONAIRE_REVENGE_MINIGAME
#DIM 回避
#DIM 正解
#DIMS 答え文字列 = "左", "右", "上", "下"
#DIM 答え

;5回、正しい選択肢を選ぶ
FOR LOCAL, 0, 5

	;答えを出しておく
	答え = RAND:4
	回避 = RAND:3

	IF 回避
		CALL COLOR_PRINTL(@"%答え文字列:答え%に避けろ！", カラー_警告)
	ELSE
		CALL COLOR_PRINTL(@"%答え文字列:答え%を避けろ！", カラー_警告)
	ENDIF

	FOR LOCAL:1, 0, VARSIZE("答え文字列")
		PRINTFORML [{LOCAL:1}] %答え文字列:(LOCAL:1)%
	NEXT

	TINPUT 1500, -1, 1

	;失敗したら強制終了
	SIF (回避 && RESULT != 答え) || (!回避 && RESULT == 答え)
		BREAK
	SIF LOCAL != 4
		PRINTFORML 贼人向这边袭来……！
		;TranslatedMark 賊はまだ襲い掛かってくる……！
NEXT

RETURN LOCAL == 5
