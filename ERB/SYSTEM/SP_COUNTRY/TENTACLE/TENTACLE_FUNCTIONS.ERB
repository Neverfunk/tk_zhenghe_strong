@TENTACLE_INIT(ARG:0)
#DIM LCOUNT
VARSET LOCAL

SIF GET_COUNTRY_FROM_ID(SP_COUNTRY_ID:特殊勢力_触手) != -1
	RETURN 0

SIF SP_COUNTRY_RANK:特殊勢力_触手 == 0
	RETURN 0

SIF GET_NEW_COUNTRY() == -1
	RETURN 0

CALL CREATE_SP_COUNTRY(特殊勢力_触手)
LOCAL = RESULT
CALL INIT_SP_COUNTRY(LOCAL)

;(触手勢力拡張パッチ)触手勢力を記録
CHANGE_Government_TENTACLE:0 = LOCAL

RETURN LOCAL

