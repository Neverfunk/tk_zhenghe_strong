@KOJO_COM_BEFORE_K163

;第一次接吻FLAG(如果没有接吻经验，将CFLAG:255设为1)
SIF TALENT:無接吻経験
	CFLAG:255 = 1
;处女FLAG(如果是处女，CFLAG:260为1)
SIF TALENT:処女
	CFLAG:260 = 1
;主人公是童贞FLAG(如果主人公是童贞，将CFLAG:261设为1)
SIF TALENT:MASTER:童貞
	CFLAG:261 = 1
	
RETURN 0

;=================================================
