@SET_SHIKIEIKI_FACE(cloth_id = -1,mood_id = -1,SP:0 = -1,SP:1 = -1,SP:2 = -1,SP:3 = -1,SP:4 = -1,SP:5 = -1)
    #DIM cloth_id
    #DIM mood_id
    #DIM SP,6
    SIF !GET_K36_IMAGE_STATUS()
        RETURN
    IF cloth_id >= 0; && 四季_衣服_id != cloth_id
        四季_衣服_id = cloth_id
        四季_face_change_flag = 1
    ENDIF

    IF mood_id >= 0 ;&& 四季_感情_id != mood_id
        四季_感情_id = mood_id
        四季_face_change_flag = 1
    ENDIF
    FOR LOCAL,0,6
        IF (SP:LOCAL) >= 0 ;&& 四季_额外_bit != SP
            IF SP:LOCAL == 四季_额外_清空
                四季_额外_bit = 0
            ENDIF
            SETBIT 四季_额外_bit,(SP:LOCAL)
            四季_face_change_flag = 1
        ENDIF
    NEXT

