		FOR LOCAL:0, 0, MAX_CITY
			SIF CITY_OWNER:(LOCAL:0) == 童子勢力
				CITY_OWNER:(LOCAL:0) = LOCAL:1
		NEXT

		FOR LOCAL:0, 0, CHARANUM
			;童子勢力に寝返ったキャラの処理
			IF CFLAG:(LOCAL:0):所属 == 童子勢力
				;自国の捕虜なら無所属に
				IF CFLAG:(LOCAL:0):捕虜先 != 0
					CFLAG:(LOCAL:0):所属 = 0
				;それ以外なら放浪状態に
				ELSE
					CFLAG:(LOCAL:0):所属 = 0
					CFLAG:(LOCAL:0):特殊状態 = 特殊状態_放浪
				ENDIF
