	;TranslatedMark %ANAME(咲夜)%はぐでんぐでんになって赤ら顔をにやつかせながら寝ている
	WAIT
	RESETCOLOR
	RETURN 0
ENDIF

;快感のあまり気絶
IF TCVAR:咲夜:52 || BASE:咲夜:気力 <= 300
	PRINTFORML 「嗯……啊咿❤呼啊……」
	;TranslatedMark 「んう……ぁふ❤　ふあぁ……」
	PRINTFORMDL %ANAME(咲夜)%四肢时不时地抽搐，恍惚地笑着，脚趾不成体统地勾起
	;TranslatedMark %ANAME(咲夜)%はくったりした手足を時々ぴくつかせて、恍惚と笑んでいる
	WAIT
	RESETCOLOR
	RETURN 0
ENDIF

;疲労による行動不能
IF TCVAR:咲夜:51
	PRINTFORML 「嗯……嗯……」
	;TranslatedMark 「う……んん……」
	PRINTFORMDL 即使%ANAME(咲夜)%睡着了，她脸上的疲倦在也未褪去
	;TranslatedMark %ANAME(咲夜)%は疲れ果てた顔で眠っている
	WAIT
	RESETCOLOR
	RETURN 0
ENDIF

;不機嫌
咲夜機嫌 = %TOSTR_EMOTION(咲夜)%
SELECTCASE 咲夜機嫌
	CASE "恨", "怒", "憤"
		PRINTFORML 「已经！回不去了%POLITE_K9("啊", "呜")%」
		;TranslatedMark 「もう！　帰%POLITE_K9("るわよ", "ります")%」
		WAIT
		RESETCOLOR
		RETURN 0
	CASE "鬱", "悲", "憂"
		PRINTFORML 「……辛苦了」
		;TranslatedMark 「……お疲れ様%POLITE_K9("ね", "です")%」
		WAIT
		RESETCOLOR
		RETURN 0
	CASE  "狂", "恐", "怯"
		PRINTFORML 「……对不起，对不起」
		;TranslatedMark 「……失礼%POLITE_K9("するわ", "します")%」
		WAIT
		RESETCOLOR
		RETURN 0
ENDSELECT

;既成事実獲得後・一回きり
IF CFLAG:咲夜:200 < 4 && TALENT:合意
	CFLAG:咲夜:200 = 4
	PRINTFORML 「皮肤好像还粘在一起……」
