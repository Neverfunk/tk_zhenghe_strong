@BUILDING_SET_INFO_ID_14()
    BUILDING_TYPE:建造物_指挥部 = 建造物_类型_军事类
    BUILDING_LEVEL:建造物_指挥部 = 建造物_等级_三级建筑
    BUILDING_LIMIT_NUM:建造物_指挥部 = 建造物_数量限制_1
    BUILDING_LIMIT_UNI:建造物_指挥部 = 0
    BUILDING_COUNTRY_ID:建造物_指挥部:特殊勢力_普通势力 = 1
    BUILDING_HAVE_SKILL:建造物_指挥部 = 0
    BUILDING_TOHO_REQ_ID:建造物_指挥部:0 = 0

@CHECK_BUILDING_FUNCTION_指挥部(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_指挥部 * 30
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_指挥部_1(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORM 因为《指挥部》的存在，
    PRINTFORML %ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%的军队武斗提高了！
    RETURNF 1
