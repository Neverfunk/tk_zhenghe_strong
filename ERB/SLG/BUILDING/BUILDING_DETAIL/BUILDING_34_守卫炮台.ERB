@BUILDING_GET_NAME_守卫炮台()
    #FUNCTIONS
    RETURNF "守卫炮台"

@BUILDING_GET_EXP_守卫炮台()
    #FUNCTIONS
    RETURNF "都市的援护射击再次强化。"

@BUILDING_SET_INFO_ID_34()
    BUILDING_TYPE:建造物_守卫炮台 = 建造物_类型_防御类
    BUILDING_LEVEL:建造物_守卫炮台 = 建造物_等级_二级建筑
    BUILDING_LIMIT_NUM:建造物_守卫炮台 = 建造物_数量限制_无
    BUILDING_LIMIT_UNI:建造物_守卫炮台 = 0
    BUILDING_COUNTRY_ID:建造物_守卫炮台:特殊勢力_普通势力 = 1
    BUILDING_HAVE_SKILL:建造物_守卫炮台 = 0
    BUILDING_TOHO_REQ_ID:建造物_守卫炮台:0 = 0

@CHECK_BUILDING_FUNCTION_守卫炮台(CITY_ID)
    #FUNCTION
    #DIM CITY_ID
    RESULT:1 = GET_CITY_BUILDING_NUM(CITY_ID,建造物_守卫炮台)
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_守卫炮台(COUNTRY_ID,CITY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    #DIM CITY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORML 因为《守卫炮台》的存在，%ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%的%CITY_NAME:CITY_ID%的守军向城下宣泄着炮火！
    RETURNF 1
