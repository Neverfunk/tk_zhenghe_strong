@UPDATE_CSV_CHARA_RESOLUTION_CONFLICT(CSVNO)
	#DIM CSVNO
	#DIM NEW_CHARA_ID
	#DIM DYNAMIC CHARA_EXIST
	CALL ADD_CHARA_FROM_CSV(CSVNO)
	NEW_CHARA_ID = RESULT
	FOR LOCAL,1,CHARANUM-1
		IF CSTR:NEW_CHARA_ID:99 == CSTR:LOCAL:99
			NO:LOCAL = NO:NEW_CHARA_ID
			CHARA_EXIST = 1
		ENDIF
	NEXT
	SIF CHARA_EXIST
		DELCHARA NEW_CHARA_ID
