@COM_ABLE291
;共通部分
CALL COM_ABLE_COMMON(291)
SIF RESULT == 0
	RETURN 0
;会いに行く・閨に呼ぶ・捕虜会話限定
SIF !GROUPMATCH(FLAG:調教モード, 0, 1, 6)
	RETURN 0
;行動可能なキャラの数を調べる
LOCAL:10 = 0
FOR LOCAL:0, 0, CHARANUM
	IF IS_AUTOSELECT(LOCAL:0)
		LOCAL:10 ++
	ENDIF
NEXT
;行動可能なキャラがいなければ不可
IF LOCAL:10 <= 0
	RETURN 0
ENDIF
RETURN 1

