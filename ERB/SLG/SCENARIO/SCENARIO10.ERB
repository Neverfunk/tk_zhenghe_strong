;-------------------------------------------------
;童子勢力の作成
;-------------------------------------------------
@CREATE_COUNTRY_DOUJI
#DIM 新勢力
#DIM 童子
新勢力 = GET_NEW_COUNTRY()
童子 = NAME_TO_CHARA("茨木童子の腕")

COUNTRY_BOSS:(新勢力)      = GET_ID(NAME_TO_CHARA("茨木童子の腕"))
COUNTRY_COLOR:(新勢力)     = 0x757575
COUNTRY_SOLDIER:(新勢力)   = 3000000
COUNTRY_IS_CLOSED:(新勢力) = 1

FOR LOCAL:0, 0, 30
	CALL ADD_SKELETON_SOLDIER(新勢力)
NEXT

CALL CHANGE_COUNTRY(NAME_TO_CHARA("茨木童子の腕"), 新勢力, 1)
CALL CHANGE_COUNTRY(NAME_TO_CHARA("萃香"),         新勢力, 1)
CALL CHANGE_COUNTRY(NAME_TO_CHARA("勇儀"),         新勢力, 1)

CALL TAKEOVER_CITY_FROM_NO(新勢力, 80)
CALL TAKEOVER_CITY_FROM_NO(新勢力, 87)
CALL TAKEOVER_CITY_FROM_NO(新勢力, 91)
CALL TAKEOVER_CITY_FROM_NO(新勢力, 95)
CALL TAKEOVER_CITY_FROM_NO(新勢力, 96)
CALL TAKEOVER_CITY_FROM_NO(新勢力, 101)

CITY_SOLDIER:80  = 50000
CITY_SOLDIER:87  = 50000
CITY_SOLDIER:91  = 50000
CITY_SOLDIER:95  = 50000
CITY_SOLDIER:96  = 50000
CITY_SOLDIER:101 = 50000

CALL CHANGE_RELATION_C_TO_C(童子, SCVAR:21, 0, 1000)

