RESULTS:5 = %LOCALS:0%(展示)

LOCAL = GET_INSERT_TARGET(MPLY:0)

SIF LOCAL == -1 || !HAS_PENIS(MPLY:0)
	RETURN 0

SELECTCASE IS_INSERT_MUTUAL(MPLY:0, LOCAL)
	CASE 1
		LOCALS:0 = 膣内放尿
	CASE 2
		LOCALS:0 = 肛内放尿
	CASEELSE
		LOCALS:0 = 放尿
ENDSELECT

