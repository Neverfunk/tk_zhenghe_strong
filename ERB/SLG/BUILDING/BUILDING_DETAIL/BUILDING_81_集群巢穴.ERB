@BUILDING_GET_NAME_集群巢穴()
    #FUNCTIONS
    RETURNF "集群巢穴"

@BUILDING_GET_EXP_集群巢穴()
    #FUNCTIONS
    RETURNF "回合末征兵数增加。"

@BUILDING_SET_INFO_ID_81()
    BUILDING_TYPE:建造物_集群巢穴 = 建造物_类型_特殊类
    BUILDING_LEVEL:建造物_集群巢穴 = 建造物_等级_一级建筑
    BUILDING_LIMIT_NUM:建造物_集群巢穴 = 建造物_数量限制_无
    BUILDING_LIMIT_UNI:建造物_集群巢穴 = 0
    BUILDING_COUNTRY_ID:建造物_集群巢穴:特殊勢力_ホフゴブリン = 1
    BUILDING_HAVE_SKILL:建造物_集群巢穴 = 0
    BUILDING_TOHO_REQ_ID:建造物_集群巢穴:0 = 0

@CHECK_BUILDING_FUNCTION_集群巢穴(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_SP_COUNTRY_LIMIT_USING(COUNTRY_ID,建造物_集群巢穴)
        RETURNF RESULT:0
    RESULT:1 = TMP_BUILDING_CNT:COUNTRY_ID:建造物_集群巢穴
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_集群巢穴(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORM 因为《集群巢穴》的存在，
    PRINTFORML 哥布林的集群巢穴里汇集了大量的哥布林……
    PRINTFORML %ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%募集到了大量士兵
    RETURNF 1
