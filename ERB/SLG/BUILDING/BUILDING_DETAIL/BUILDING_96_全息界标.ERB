@BUILDING_GET_NAME_全息界标()
    #FUNCTIONS
    RETURNF "全息界标"

@BUILDING_GET_EXP_全息界标()
    #FUNCTIONS
    RETURNF "回合末征兵数增加。"

@BUILDING_SET_INFO_ID_96()
    BUILDING_TYPE:建造物_全息界标 = 建造物_类型_特殊类
    BUILDING_LEVEL:建造物_全息界标 = 建造物_等级_一级建筑
    BUILDING_LIMIT_NUM:建造物_全息界标 = 建造物_数量限制_无
    BUILDING_LIMIT_UNI:建造物_全息界标 = 0
    BUILDING_COUNTRY_ID:建造物_全息界标:特殊勢力_狂信者 = 1
    BUILDING_HAVE_SKILL:建造物_全息界标 = 0
    BUILDING_TOHO_REQ_ID:建造物_全息界标:0 = 0

@CHECK_BUILDING_FUNCTION_全息界标(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_SP_COUNTRY_LIMIT_USING(COUNTRY_ID,建造物_全息界标)
        RETURNF RESULT:0
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_全息界标
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_全息界标(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORM 因为《全息界标》的存在，
    PRINTFORML 无数人倾倒于神权的广袤无边
    PRINTFORML %ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%募集到了大量信徒！
    RETURNF 1
