    CALL SET_ONOZUKA_FACE(小町_衣服_普通,小町_感情_普通,小町_额外表情_无)
    CALL K35_ONOZUKA_SAY_IMAGE(@"「这个简单，我指给你看。」")
    CALL K35_ONOZUKA_SAY_IMAGE(@"「食堂在这边，浴场在那边，不过这里的享受的话随时会被%K36_CALL_四季大人()%抓到呢。」")
    CALL K35_ONOZUKA_SAY_IMAGE(@"「要享乐的话还是得去外面，人里那里也有酒屋和浴场……」")
    CALL K35_ONOZUKA_SAY_IMAGE(@"「房间的话去找宿舍区的管理员说一声就行了，她会给你分配的。」")
    CALL SET_ONOZUKA_FACE(小町_衣服_普通,小町_感情_开心,小町_额外表情_无)
    CALL K35_ONOZUKA_SAY_IMAGE(@"「南北通透，独立卫浴，还有双人床，最挑剔的鬼来了都说好，更何况是人呢？哈哈哈哈……」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"（……？）")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%无奈地看着爽朗大笑着的%ANAME(小町_ID)%")
    CALL SET_ONOZUKA_FACE(小町_衣服_普通,小町_感情_普通,小町_额外表情_无)
    CALL K35_ONOZUKA_SAY_IMAGE(@"「至于工作的地方嘛……」")
    CALL K35_ONOZUKA_SAY_IMAGE(@"「%K36_CALL_四季大人()%那里就是正经的办公区啦，但是千万别去那里。」")
    CALL K35_ONOZUKA_SAY_IMAGE(@"「%K36_CALL_四季大人()%是个不顾他人的工作狂。去那里的唯一结局就是被当成苦力榨干。」")
    CALL K35_ONOZUKA_SAY_IMAGE(@"「%ANAME(MASTER)%去我那里好了，我那里还蛮大的。」")
    CALL K35_ONOZUKA_SAY_IMAGE(@"「明天就来报道吧，我会把你的工作告诉你的。」")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%和%ANAME(小町_ID)%聊了一会")
    CALL K36_一段时间后_IMAGE
    CALL K35_ONOZUKA_SAY_IMAGE(@"「好了好了，我要去喝酒了，要一起去吗？」")
    CALL K35_ONOZUKA_SAY_IMAGE(@"「要熟悉一下住的地方？也是呢。」")
    CALL K35_ONOZUKA_SAY_IMAGE(@"「那么下次一定要去，我请你！」")
    CBGCLEAR
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(小町_ID)%一瞬间走到了很远的地方")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"（总觉得不太靠谱的样子呢）",1)
    IF !CFLAG:小町_ID:面識
        LOCAL = LINECOUNT
        CALL KOJO_DAILY_START("死神如风！", 小町_ID)
        CLEARLINE LINECOUNT - LOCAL
        CALL SET_ONOZUKA_FACE(小町_衣服_普通,小町_感情_普通,小町_额外表情_无)
        CALL K35_ONOZUKA_SAY_IMAGE(@"「哦对了都没请教你的名字呢！」")
