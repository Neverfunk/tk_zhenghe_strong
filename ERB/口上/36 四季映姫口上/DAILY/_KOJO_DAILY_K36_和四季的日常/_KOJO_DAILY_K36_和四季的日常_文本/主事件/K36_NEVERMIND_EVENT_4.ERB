@K36_NEVERMIND_EVENT_4_EXIST
    RETURN 1

@K36_NEVERMIND_EVENT_4_RATE
    IF DAY - K36_上次色色日期 <= 3
        RETURN 2 + MIN((DAY - K36_和四季的日常_主事件_上次触发日期:4),10)
    ELSE
        RETURN 1
    ENDIF

@K36_NEVERMIND_EVENT_4_STATUS
    RETURN 1

@K36_NEVERMIND_EVENT_4
    CALL K36_NORMAL_SENTENCE_IMAGE(@"夜里，宫殿内的办事处依然灯火通明")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"因为边境的突发事件，%ANAME(MASTER)%和%ANAME(四季_ID)%两人不得不通宵处理文件",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_普通,四季_额外_清空)
    CALL KOJO_36_IMAGE
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%为两人各泡了一杯茶，随后坐了下来，准备开始工作")
    CALL K36_NORMAL_SENTENCE_IMAGE(@"本来已经十分默契的两人不需要再说什么别的了",1)
    CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_开心,四季_额外_清空,四季_额外_害羞)
    CALL KOJO_36_IMAGE
    CALL K36_NORMAL_SENTENCE_IMAGE(@"但是突然间两人的目光对上了",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%有些害羞的朝%ANAME(MASTER)%笑了笑",1)
    CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%决定")
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"用%K36_CALL_四季大人()%泡的茶提神",1)
    CALL K36_NORMAL_SENTENCE_IMAGE_BUTTON(@"用%K36_CALL_四季大人()%提神！",0)
    INPUT
    CLEARLINE 1
    IF RESULT != 0
        CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%把%ANAME(四季_ID)%泡的茶一饮而尽")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「好喝！」",1)
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_生气,四季_额外_清空,四季_额外_害羞)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「真是的，不是给你这么喝的啊！」")
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_爱意,四季_额外_清空,四季_额外_害羞)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「慢慢来也挺好的嘛……」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「……%ANAME(MASTER)%喜欢的话，我待会再给你泡……」")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"两人度过了一个劳累的夜晚")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"不过因为在意的人就在身边，两人都很精神",1)
        CALL K36_ADD_四季好感度
    ELSE
        CALL K36_NORMAL_SENTENCE_IMAGE(@"「这里也想要%K36_CALL_映姬()%帮忙提神呢！」",1)
        CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%如同痴汉一般展示着隆起的帐篷")
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_生气,四季_额外_清空,四季_额外_害羞)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「变！变态！」")
        IF DAY - K36_上次色色日期 <= 3
            CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_生气,四季_额外_清空,四季_额外_害羞,四季_额外_生气)
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「不是最近才做过嘛……所以不行！」")
        ELSE
            CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_爱意,四季_额外_清空,四季_额外_害羞)
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「最近是没怎么做呢……」")
            CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_生气,四季_额外_清空,四季_额外_害羞)
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「但是晚上还要工作呢……所以不行！」")
        ENDIF
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「不行就是不行！」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「所以说不行啊，%K36_CALL_MASTER()%不要再这样看着我了！」")
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_发情,四季_额外_清空,四季_额外_害羞)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「唔……就，就一次！我帮%K36_CALL_MASTER()%……」")
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「而且要是拖累了后面的工作的话，就绝对没有下一次了！」")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%跪在%ANAME(MASTER)%面前，拉开了裤子的拉链")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"一瞬间%ANAME(MASTER)%的巨根就弹了出来",1)
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_发情,四季_额外_清空,四季_额外_害羞重)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「这，这么大……」")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%用手轻轻撸动着肉棒")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"舌头慢慢在鬼头上转动着",1)
        CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_接吻,四季_额外_清空,四季_额外_害羞重,四季_额外_爱心)
        CALL K36_SHIKIEIKI_SAY_IMAGE(@"「唔……♥」")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"似乎是为了加快速度")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%努力张大嘴把%ANAME(MASTER)%的雄根含住了",1)
        CALL K36_NORMAL_SENTENCE_IMAGE(@"平常那张说教了无数人的嘴现在却在满足着%ANAME(MASTER)%下流的欲望")
        CALL K36_NORMAL_SENTENCE_IMAGE(@"虽然是很淫荡的行为，但要强的%ANAME(四季_ID)%依然尽全力的让%ANAME(MASTER)%感到愉悦",1)
        CALL K36_NORMAL_SENTENCE_IMAGE(@"在尽力的仕奉下，%ANAME(MASTER)%很快感到自己抵达了极限")
        IF DAY - K36_上次色色日期 <= 3
            CALL K36_NORMAL_SENTENCE_IMAGE(@"被性欲缠身的%ANAME(MASTER)%下意识地按住了%ANAME(四季_ID)%的头",1)
            CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_震惊,四季_额外_清空,四季_额外_害羞重,四季_额外_爱心)
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「唔！……唔♥！」")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%下意识地反抗着，但是已经来不及了")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%在%ANAME(四季_ID)%喉头释放了自己的欲望",1)
            CALL K36_一段时间后_IMAGE
            CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_爱意,四季_额外_清空,四季_额外_害羞重,四季_额外_爱心,四季_额外_吐气)
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「咳……咳……」")
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「哈……啊♥……哈♥」")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%小口喘着气")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"「%K36_CALL_映姬()%！没，没事吧。我刚刚……」",1)
            CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_生气,四季_额外_清空,四季_额外_害羞重,四季_额外_爱心,四季_额外_吐气)
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「真！是！的！怎么那么猴急啊！」")
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「好，好了！赶紧工作吧！」")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"害怕继续惹怒%ANAME(四季_ID)%的%ANAME(MASTER)%赶紧开始了工作",1)
            CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_发情,四季_额外_清空,四季_额外_害羞重,四季_额外_爱心,四季_额外_吐气)
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「（这么浓♥……这家伙怎么一直那么有欲望啊♥）」")
            CALL K36_ADD_四季敏感度
            CALL K36_FUCK(1,,"口")
        ELSE
            CALL K36_NORMAL_SENTENCE_IMAGE(@"「唔……%K36_CALL_映姬()%……要射了！」",1)
            CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_接吻,四季_额外_清空,四季_额外_害羞重,四季_额外_爱心,四季_额外_吐气)
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「唔！……唔♥！」")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(MASTER)%下意识地把雄根退了出来",1)
            CALL K36_NORMAL_SENTENCE_IMAGE(@"一瞬间就在%ANAME(四季_ID)%的脸上释放了欲望")
            CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_接吻,四季_额外_清空,四季_额外_害羞,四季_额外_吐气,四季_额外_精液上)
            CALL KOJO_36_IMAGE
            CALL K36_NORMAL_SENTENCE_IMAGE(@"白浊污染了%ANAME(四季_ID)%纯洁的脸庞",1)
            CALL K36_一段时间后_IMAGE
            CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_接吻,四季_额外_清空,四季_额外_害羞,四季_额外_吐气,四季_额外_精液上)
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「哈……啊♥……哈♥」")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%小口喘着气")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"「唔……%K36_CALL_映姬()%……我……」",1)
            CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_维持上次,四季_额外_清空,四季_额外_害羞,四季_额外_吐气,四季_额外_精液上)
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「满，满足了吧？」")
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「今天先工作吧。」")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"回到了座位上")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%用手指擦拭着脸上的精液",1)
            CALL K36_NORMAL_SENTENCE_IMAGE(@"似乎不知道丢到哪里")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"犹豫了一下，把沾满精液的手指放入嘴中吮吸着……",1)
            CALL SET_SHIKIEIKI_FACE(四季_衣服_常服,四季_感情_维持上次,四季_额外_清空,四季_额外_害羞,四季_额外_吐气,四季_额外_爱心)
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「好……好浓的味道♥」")
            CALL K36_SHIKIEIKI_SAY_IMAGE(@"「感觉整个人都♥♥♥」")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"%ANAME(四季_ID)%假装没有感到嘴角残留的精液")
            CALL K36_NORMAL_SENTENCE_IMAGE(@"就这样在%ANAME(MASTER)%的气味中度过了一晚",1)
            CALL K36_ADD_四季好感度
            CALL K36_ADD_四季敏感度
            CALL K36_FUCK(1,,"口")
        ENDIF
    ENDIF
