@CHECK_BUILDING_FUNCTION_人偶剧场(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_CHARA_LIMIT_USING(COUNTRY_ID,建造物_人偶剧场)
        RETURNF RESULT:0
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_人偶剧场
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_人偶剧场(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORM 因为《人偶剧场》的存在，
    PRINTFORML %ANAME(14)%精湛的表演吸引了大量想要参军的青年
    PRINTFORML %ANAME(GET_COUNTRY_BOSS(COUNTRY_ID))%势力获得了额外的征兵
    RETURNF 1
