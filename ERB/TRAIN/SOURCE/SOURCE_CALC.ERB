@CALC_SOURCE0(ARG:0)
LOCAL:0 = SOURCE:(ARG:0):快Ｃ
LOCAL:1 = SOURCE:(ARG:0):快Ｃ
LOCAL:2 = SOURCE:(ARG:0):快Ｃ / 5
LOCAL:5 = SOURCE:(ARG:0):快Ｃ

LOCAL:0 = CALC_SOURCE_SENSE_BASE(ABL:(ARG:0):Ｃ感, LOCAL:0)
LOCAL:1 = CALC_SOURCE_SENSE_BASE(ABL:(ARG:0):Ｃ感, LOCAL:1)

;欲情による共通補正
LOCAL:0 = CALC_SOURCE_SENSE_ESTRUS(ARG:0, LOCAL:0)

SELECTCASE ABL:(ARG:0):欲望
	CASE 0
		TIMES LOCAL:1, 0.10
		TIMES LOCAL:2, 1.00
	CASE 1
		TIMES LOCAL:1, 0.15
		TIMES LOCAL:2, 0.85
	CASE 2
		TIMES LOCAL:1, 0.20
		TIMES LOCAL:2, 0.70
	CASE 3
		TIMES LOCAL:1, 0.25
		TIMES LOCAL:2, 0.40
	CASE 4
		TIMES LOCAL:1, 0.30
		TIMES LOCAL:2, 0.30
	CASEELSE
		LOCAL:1 = LOCAL:1 * ((ABL:(ARG:0):欲望 - 5) * 5 + 40) / 100
		TIMES LOCAL:2, 0.10
ENDSELECT

IF TALENT:(ARG:0):Ｃ敏感
	TIMES LOCAL:0, 1.5
	TIMES LOCAL:1, 2.00
	TIMES LOCAL:5, 1.50
ENDIF

IF TALENT:(ARG:0):Ｃ鈍感
	TIMES LOCAL:0, 0.75
	TIMES LOCAL:1, 0.50
	TIMES LOCAL:5, 0.75
ENDIF

IF GETBIT(TALENT:(ARG:0):淫乱系, 素質_淫乱_淫核)
	TIMES LOCAL:0, 1.50
	TIMES LOCAL:1, 1.20
	TIMES LOCAL:2, 0.50
	TIMES LOCAL:5, 1.50
ENDIF

;媚薬使用中
IF TCVAR:(ARG:0):60
	TIMES LOCAL:0, 1.5
	TIMES LOCAL:1, 1.5
	TIMES LOCAL:5, 1.50
ENDIF

;桃源香使用中
IF TFLAG:69
	TIMES LOCAL:0, 1.25
	TIMES LOCAL:1, 1.5
	TIMES LOCAL:5, 1.25
ENDIF

;体力0による行動不能
