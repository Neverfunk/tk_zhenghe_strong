    #DIM COUNTRY_ID
    RESULT:1 = TMP_BUILDING_CNT:COUNTRY_ID:建造物_スパコン
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

