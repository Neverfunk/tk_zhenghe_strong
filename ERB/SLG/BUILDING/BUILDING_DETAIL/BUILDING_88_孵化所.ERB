@BUILDING_GET_NAME_孵化所()
    #FUNCTIONS
    RETURNF "孵化所"

@BUILDING_GET_EXP_孵化所()
    #FUNCTIONS
    RETURNF "《自然培养皿》的效果翻倍。"

@BUILDING_SET_INFO_ID_88()
    BUILDING_TYPE:建造物_孵化所 = 建造物_类型_特殊类
    BUILDING_LEVEL:建造物_孵化所 = 建造物_等级_二级建筑
    BUILDING_LIMIT_NUM:建造物_孵化所 = 建造物_数量限制_1
    BUILDING_LIMIT_UNI:建造物_孵化所 = 0
    BUILDING_COUNTRY_ID:建造物_孵化所:特殊勢力_触手 = 1
    BUILDING_HAVE_SKILL:建造物_孵化所 = 0
    BUILDING_TOHO_REQ_ID:建造物_孵化所:0 = 0

@CHECK_BUILDING_FUNCTION_孵化所(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_SP_COUNTRY_LIMIT_USING(COUNTRY_ID,建造物_孵化所)
        RETURNF RESULT:0
    RESULT:1 = TMP_BUILDING_EXIST_FLAG:COUNTRY_ID:建造物_孵化所
    IF RESULT:1 > 0
        RESULT:0 = 1
    ENDIF
    RETURNF RESULT:0

@BUILDING_TEXT_孵化所(COUNTRY_ID)
    #FUNCTION
    #DIM COUNTRY_ID
    SIF !CHECK_BUILDING_TEXT_OUTPUT(COUNTRY_ID)
        RETURNF 0
    BUILDING_TEXT = PRINT_BUILDING_TEXT_TITLE_F()
    PRINTFORM 因为《孵化所》的存在，
    PRINTFORML 《自然培养皿》的效果翻倍了
    RETURNF 1
